{
    "name": "helpscout/api",
    "type": "library",
    "description": "API client for the Help Scout API",
    "keywords": ["api", "api-client", "helpscout"],
    "homepage": "http://www.helpscout.net",
    "license": "MIT",
    "require": {
        "php": ">=5.3.2",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "HelpScout\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
